// USER
CCP/ /SET.LEVEL = 5

// [FR] User-agent des requetes HTTP (PAR DEFAUT) --> la meme que le ping
// [EN] HTTP request user-agent (BY DEFAULT) --> like ping
SET/ CPC_SYS.NET.USERAGENT = %CPC_SYS.NET.ICMP%

// [FR] Version du HTTP (PAR DEFAUT)
// [EN] HTTP version (BY DEFAULT)
SET/ CPC_SYS.NET.HTTP = HTTP/1.0




